<!DOCTYPE html>
<html>

<head>
  <title>Login</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="../public/css/index.css" rel="stylesheet" type="text/css">
  <script type="text/javascript" src="../public/js/clience-validation.js"></script>
</head>

<body>
  <header>
    <div class="container"></div>
  </header>
  <main>
    <div class="container">
      <div class="login-form">
        <form action="/" method="post">
          <h1>Login</h1>
          <div class="input-box"><i></i>
            <input id="username" type="text" placeholder="Username">
          </div>
          <div class="input-box"><i></i>
            <input id="password" type="password" placeholder="Password">
          </div>
          <div class="btn-box">
            <button type="submit" onclick="login_validate()">login</button>
          </div>
        </form>
      </div>
    </div>
  </main>
  <footer>
    <div class="container"></div>
  </footer>
</body>

</html>
